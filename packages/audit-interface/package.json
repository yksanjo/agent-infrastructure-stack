{
  "name": "@agent-infra/audit-interface",
  "version": "1.0.0",
  "description": "Human-in-the-loop audit interface with 5-second comprehension design",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
